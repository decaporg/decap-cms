  "module": "dist/esm/index.js",
  "main": "dist/netlify-cms-widget-markdown.js",
  "license": "MIT",
  "keywords": [
    "netlify",
    "netlify-cms",
    "widget",
    "markdown",
    "editor"
  ],
  "sideEffects": false,
  "scripts": {
