    "remark-stringify": "^3.0.1",
    "slate": "^0.34.0",
    "slate-edit-list": "^0.11.3",
    "slate-edit-table": "^0.15.1",
    "slate-plain-serializer": "^0.5.15",
    "slate-react": "0.12.9",
