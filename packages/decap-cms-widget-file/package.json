  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "decap-cms-ui-default": "^3.0.0",
    "immutable": "^3.7.6",
    "prop-types": "^15.7.2",
    "react": "^19.1.0",
    "react-immutable-proptypes": "^2.1.0",
    "uuid": "^8.3.2"
