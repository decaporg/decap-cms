  "bugs": "https://github.com/decaporg/decap-cms/issues",
  "license": "MIT",
  "module": "dist/esm/index.js",
  "main": "dist/decap-cms-analytics-simple.js",
  "keywords": [
    "decap-cms",
    "analytics",
    "simple"
  ],
  "sideEffects": false,
  "scripts": {
