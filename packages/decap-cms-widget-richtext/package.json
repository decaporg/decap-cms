    "lucide-react": "^0.331.0",
    "platejs": "^49.2.21",
    "rehype-parse": "^6.0.0",
    "rehype-remark": "^8.0.0",
    "rehype-stringify": "^7.0.0",
    "remark-parse": "^6.0.3",
    "remark-rehype": "^4.0.0",
    "remark-slate": "^1.8.6",
    "remark-slate-transformer": "^0.7.4",
    "remark-stringify": "^6.0.4",
    "unified": "^9.0.0"
