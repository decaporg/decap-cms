    "react-select": "^4.0.0",
    "react-window": "^1.8.5"
  },
  "peerDependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.10.5",
    "decap-cms-lib-widgets": "^3.0.0",
    "decap-cms-ui-default": "^3.0.0",
    "immutable": "^3.7.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.4 || ^17.0.0",
    "uuid": "^3.3.2"
