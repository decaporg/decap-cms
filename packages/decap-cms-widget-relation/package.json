    "@emotion/styled": "^11.11.0",
    "decap-cms-lib-widgets": "^3.0.0",
    "decap-cms-ui-default": "^3.0.0",
    "immutable": "^3.7.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "uuid": "^8.3.2"
