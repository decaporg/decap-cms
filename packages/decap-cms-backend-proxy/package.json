{
  "name": "decap-cms-backend-proxy",
  "description": "Proxy backend for Decap CMS",
  "version": "3.2.0",
  "repository": "https://github.com/decaporg/decap-cms/tree/main/packages/decap-cms-backend-proxy",
  "bugs": "https://github.com/decaporg/decap-cms/issues",
  "license": "MIT",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "main": "dist/decap-cms-backend-proxy.js",
  "keywords": [
    "decap-cms",
    "backend"
  ],
  "sideEffects": false,
  "scripts": {
    "develop": "npm run build:esm -- --watch",
    "build": "cross-env NODE_ENV=production webpack",
    "build:esm": "run-p build:types build:babel",
    "build:babel": "cross-env NODE_ENV=esm babel src --out-dir dist/esm --ignore \"**/__tests__\" --root-mode upward --extensions \".js,.jsx,.ts,.tsx\"",
    "build:types": "tsc"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "decap-cms-lib-util": "^3.0.0",
    "decap-cms-ui-default": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^19.1.0"
  }
}
