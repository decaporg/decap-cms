    "react-textarea-autosize": "^8.0.0",
    "react-transition-group": "^4.4.5",
    "react-spring": "^9.7.3",
    "keycode": "^2.2.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
