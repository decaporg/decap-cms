    "moment": "^2.29.4",
    "decap-cms-app": "^3.0.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-btn": "^1.4.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^9.0.1",
    "smooth-scroll": "^16.1.3"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.11.0",
    "babel-plugin-prismjs": "^2.1.0",
    "babel-preset-gatsby": "^3.13.0",
    "eslint": "^8.56.0",
